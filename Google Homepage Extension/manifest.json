{
  "manifest_version": 2,

  "name": "Custom Google Homepage",
  "description": "This extension shows a Google Images search result for the current page",
  "version": "1.0",

  "content_scripts": [ 
  {
   "matches": ["https://www.google.com/*"],
   "css": ["main.css"]
   }
   ],
   
  "browser_action": { 
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "default_title": "My custom google page!"
  }
}